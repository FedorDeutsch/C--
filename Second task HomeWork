#include <iostream>
using namespace std;

void operation(int arr[]) {

    for (int i = 0; i < 5; i++) {
        cout << arr[i] * 2 << endl;

    }
    cout << endl;
}

int main() {
    int arr[5] = { 1, 2, 3, 4, 5 };
    for (int i = 0; i < 5; i++) {
        cout << arr[i] << endl;

    }

    operation(arr);

    return 0;
}
