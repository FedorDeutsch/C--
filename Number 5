#include <iostream>
using namespace std;

int main() {
    int n;

    cout << "Введите число n: ";
    cin >> n;

    if (n <= 0) {
        cout << "Ошибка: Число n должно быть положительным." << endl;
        return 1;
    }

    cout << "Первые " << n << " чисел Фибоначчи:" << endl;

    int fib[n];
    fib[0] = 0;
    fib[1] = 1;

    cout << fib[0] << " " << fib[1] << " ";

    for (int i = 2; i < n; ++i) {
        fib[i] = fib[i - 1] + fib[i - 2];
        cout << fib[i] << " ";
    }

    cout << endl;
    return 0;
}
