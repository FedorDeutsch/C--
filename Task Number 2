#include <iostream>
using namespace std;

void operation(int& a, int& b)
{
    int base;

    base = a;
    a = b;
    b = base;

    cout << "A: " << a << endl;
    cout << "B: " << b << endl;
    
}

int main() 
{   
    int a, b;
    a = 10;
    b = 20;
    
    cout << "A: " << a << endl;
    cout << "B: " << b << endl;

    operation(a,b);

    return 0;
}
